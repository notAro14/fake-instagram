{
  "name": "instagram-like",
  "version": "1.0.0",
  "description": "Instagram like application",
  "private": true,
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/styleMock.js",
      "\\.(png)$": "<rootDir>/src/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@hookform)/)"
    ]
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "scripts": {
    "analyze": "webpack-bundle-analyzer stats.json dist",
    "build": "rm -rf dist/ && npm-run-all build:*",
    "build:client": "NODE_ENV=production webpack --config webpack/webpack.prod.js --profile --json > stats.json",
    "build:server": "NODE_ENV=production webpack --config webpack/webpack.server.js",
    "dev:client": "webpack serve --config webpack/webpack.dev.js",
    "dev:server": "NODE_ENV=development webpack --config webpack/webpack.server.js",
    "format": "prettier --write .",
    "lint": "eslint --ext .jsx,.js src/ webpack/",
    "serve": "NODE_ENV=production node dist/server",
    "start": "rm -rf dist/ && npm-run-all --parallel dev:*",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "update": "yarn upgrade-interactive --latest",
    "postinstall": "husky install"
  },
  "keywords": [
    "instagram",
    "react",
    "mern"
  ],
  "author": "Aro Andriamaro",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.2.6",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "5.3.1",
    "husky": "5.1.3",
    "jest": "^26.6.3",
    "lint-staged": ">=10.5.3",
    "msw": "^0.28.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.3.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.2",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.1"
  },
  "dependencies": {
    "@hookform/error-message": "^0.0.5",
    "@hookform/resolvers": "^1.3.5",
    "@loadable/component": "^5.15.0",
    "@reach/menu-button": "^0.15.1",
    "bcrypt": "^5.0.1",
    "cookie-parser": "^1.4.5",
    "core-js": "3.13.1",
    "cors": "^2.8.5",
    "date-fns": "^2.22.1",
    "dotenv": "^8.2.0",
    "emoji-mart": "^3.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mini-css-extract-plugin": "^1.6.0",
    "mongoose": "^5.12.12",
    "mongoose-unique-validator": "^2.0.3",
    "multer": "^1.4.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.3",
    "react-hook-form": "^6.15.5",
    "react-hot-toast": "^1.0.2",
    "react-icons": "^4.2.0",
    "react-modal": "^3.13.1",
    "react-query": "^3.16.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2",
    "webpack-shell-plugin-next": "^2.2.2",
    "yup": "^0.32.9"
  }
}
