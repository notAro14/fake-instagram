{
  "name": "instagram-like",
  "version": "1.0.0",
  "description": "Instagram like application",
  "private": true,
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "scripts": {
    "analyze": "webpack-bundle-analyzer stats.json dist",
    "build": "rm -rf dist/ && npm-run-all build:*",
    "build:client": "NODE_ENV=production webpack --config webpack/webpack.prod.js --profile --json > stats.json",
    "build:server": "NODE_ENV=production webpack --config webpack/webpack.server.js",
    "dev:client": "webpack serve --config webpack/webpack.dev.js",
    "dev:server": "NODE_ENV=development webpack --config webpack/webpack.server.js",
    "format": "prettier --write .",
    "lint": "eslint --ext .jsx,.js src/ webpack/",
    "serve": "NODE_ENV=production node dist/server",
    "start": "rm -rf dist/ && npm-run-all --parallel dev:*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "update": "yarn upgrade-interactive --latest",
    "postinstall": "husky install"
  },
  "keywords": [
    "instagram",
    "react",
    "mern"
  ],
  "author": "Aro Andriamaro",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.10",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.4",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.2",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "html-webpack-harddisk-plugin": "^2.0.0",
    "html-webpack-plugin": "5.3.1",
    "husky": "5.1.3",
    "jest": "^26.6.3",
    "lint-staged": ">=10.5.3",
    "msw": "^0.28.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^5.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "webpack": "^5.25.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1"
  },
  "dependencies": {
    "@hookform/error-message": "^0.0.5",
    "@hookform/resolvers": "^1.3.5",
    "@loadable/component": "^5.14.1",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "core-js": "3.9.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "emoji-mart": "^3.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mini-css-extract-plugin": "^1.3.9",
    "mongoose": "^5.12.0",
    "mongoose-unique-validator": "^2.0.3",
    "multer": "^1.4.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-file-base64": "^1.0.3",
    "react-hook-form": "^6.15.5",
    "react-icons": "^4.2.0",
    "react-modal": "^3.12.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1",
    "webpack-merge": "^5.7.3",
    "webpack-node-externals": "^2.5.2",
    "webpack-shell-plugin-next": "^2.1.2",
    "yup": "^0.32.9"
  }
}
